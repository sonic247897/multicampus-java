package api.util;

import java.util.HashSet;

// 집합, 중복을 허용하지 않는다.
public class HashSetTest {
	public static void main(String[] args) {
		HashSet<String> set = new HashSet<String>();
		set.add("java");
		set.add("jdbc");
		set.add("oracle");
		set.add("html");
		set.add("css");
		// 중복된 데이터는 하나로 친다.
		set.add("java");
		System.out.println("사이즈=>"+set.size());
	}

}
