package work.one;

import oracle.sql.DATE;

public class CustomerDTO {
	private String id;
	private int pass;
	private String name;
	private DATE regdate;
	private int point;
	private String addr;
	
	public CustomerDTO() {
		
	}

	// insert¿ë
	public CustomerDTO(String id, int pass, String name, DATE regdate, int point, String addr) {
		super();
		this.id = id;
		this.pass = pass;
		this.name = name;
		this.regdate = regdate;
		this.point = point;
		this.addr = addr;
	}
	
	

	
	
}
