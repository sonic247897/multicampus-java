	public static void main(String[] args) {
		try {
			FileWriter fw = new FileWriter("src/data/output.txt");
		// FileNotFoundException이 아니라 IOException인 이유?
		} catch (IOException e) {
			e.printStackTrace();
		}
		
	}
}
